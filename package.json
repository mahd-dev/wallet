{
  "name": "live",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "start": "remix-serve ./build/server/index.js",
    "build": "run-s build:*",
    "build:id": "echo -e \"export const BUILD_ID = \\\"$(tar -cf - ./app ",
    "build:lang": "paraglide-js compile --project ./project.inlang --outdir ./lang/gen",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --platform=node --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "build:ui": "remix vite:build",
    "build:api": "cd api && vite --config vite.config.ts build",
    "dev": "run-p dev:*",
    "dev:id": "chokidar \"app/**\" -c 'echo -e \"export const BUILD_ID = \\\"$(tar -cf - ./app",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --platform=node --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "dev:serve": "vite-node ./server.js",
    "dev:schema": "vite-node api/schemawatch.ts",
    "gql": "vite-node api/schemagen.ts && graphql-codegen",
    "gql:watch": "graphql-codegen -w",
    "lint": "run-s lang:lint eslint",
    "eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint ./app",
    "db:status": "prisma migrate status",
    "db:migrate": "prisma migrate dev --name",
    "db:reset": "prisma migrate reset",
    "db:deploy": "prisma migrate deploy",
    "db:diff": "prisma migrate diff",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:execute": "prisma db execute",
    "db:generate": "prisma generate",
    "db:validate": "prisma validate",
    "db:format": "prisma validate",
    "db:studio": "prisma studio",
    "typecheck": "tsc",
    "prepare": "husky",
    "lang:validate": "inlang validate --project ./project.inlang",
    "lang:lint": "inlang lint --project ./project.inlang",
    "lang:auto": "inlang machine translate --project ./project.inlang",
    "lang:edit": "inlang open editor --project ./project.inlang",
    "postinstall": "run-s build:lang"
  },
  "prisma": {
    "schema": "schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.679.0",
    "@aws-sdk/lib-storage": "^3.679.0",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@graphile/pg-aggregates": "^0.1.1",
    "@graphile/postgis": "^0.2.0",
    "@graphile/subscriptions-lds": "^4.13.0",
    "@headlessui/react": "^2.2.0",
    "@inlang/paraglide-js-adapter-vite": "^1.2.40",
    "@livekit/components-react": "^2.6.7",
    "@livekit/components-styles": "^1.1.4",
    "@livekit/egress-sdk": "^0.2.0",
    "@prisma/client": "^5.21.1",
    "@react-pdf/renderer": "^4.3.0",
    "@remix-pwa/sw": "^3.0.10",
    "@remix-pwa/worker-runtime": "^2.1.4",
    "@remix-run/express": "^2.13.1",
    "@remix-run/node": "^2.15.3",
    "@remix-run/react": "^2.15.3",
    "@remix-run/router": "^1.22.0",
    "@tabler/icons-react": "3.18.0",
    "array.prototype.tosorted": "^1.1.4",
    "chart.js": "^4.4.8",
    "compression": "^1.7.4",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-jwt": "^8.4.1",
    "framer-motion": "^11.11.10",
    "graphile-utils": "^4.13.0",
    "graphql-ws": "^5.16.0",
    "html2canvas": "^1.4.1",
    "isbot": "^5.1.17",
    "jotai": "^2.10.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.0",
    "jwks-rsa": "^3.1.0",
    "konsta": "^4.0.1",
    "livekit-client": "^2.5.10",
    "livekit-server-sdk": "^2.7.2",
    "lucide-react": "^0.487.0",
    "mime": "^4.0.4",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.7",
    "nodemailer": "^6.10.0",
    "postgraphile": "^4.13.0",
    "postgraphile-index-to-unique-constraint-plugin": "^1.0.1",
    "postgraphile-plugin-connection-filter": "^2.3.0",
    "postgraphile-plugin-connection-filter-postgis": "^1.0.0-alpha.6",
    "postgraphile-plugin-many-create-update-delete": "^1.0.7",
    "postgraphile-upsert-plugin": "^3.3.0",
